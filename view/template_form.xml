<data>
     <xpath expr="/form/notebook/page[@id='general']/field[@name='products']" position="before">
        <label name="scrap_package"/>
        <field name="scrap_package"/>
    </xpath>

    <xpath expr="/form/notebook" position="inside">
      <page id="scrap" string="SCRAP">
          <label name="scrap_category"/>
          <field name="scrap_category"/>
          <newline/>
          <field name="scrap_template_lines" colspan="4"/>
      </page>
    </xpath>
</data>
